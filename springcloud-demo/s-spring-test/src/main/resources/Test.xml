<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="userEntity" class="com.spring.bean.UserEntity">
        <property name="username" value="qiao can" />
        <property name="age" value="30"/>
        <property name="bank" ref="bank" />
    </bean>

    <bean id="bank" class="com.spring.bean.Bank">
        <property name="account" value="3030" />
        <property name="price" value="3000"/>
    </bean>

</beans>